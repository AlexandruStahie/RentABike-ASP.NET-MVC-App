@using BD_Project.Models;
@model List<Comenzi>
@{
    ViewBag.Title = "ListaCoemzniActive";
}
<h2>Lista Comenzi Active</h2>

@foreach (var x in Model)
{
    <p>
        <div class="textedit">

            <a href="/Comenzi/DetaliiComanda/@x.IdComenzi"> @x.TimpStart  ->  @x.TimpStop </a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <a href="/Comenzi/StergeComanda/@x.IdComenzi">Sterge</a>

        </div>
    </p>

}